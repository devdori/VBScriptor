DELAY 50
RESULT = K_ReadFunction
DCV
Answer "DCV"
End Sub

[STEP41]
S0 = 5000
D0 = Power Latch Off Check (오류)
Sub Step41()
MUX "OSW", 1
RESULT = K_WriteFunction(3, "OFF")	' PWL Relay Off
RESULT = K_WriteFunction(4, "OFF")	' NSLP Relay Off
'RESULT = K_ReadFunction
DELAY 100
DCV
Answer "DCV"
End Sub

[STEP42]
S0 = 1001
D0 = INH 출력 확인
Sub Step42()
MUX "OSW", 1
'SetV 0
'Switch "VB", "GND", 0
'Switch "IG", "POW", 0
'Switch "KLin", "MCU_KLINE", 0
'Switch "VB", "GND", 1
'Delay 300
'SwitchOffAll
'Delay 300
SetV 13.5
Switch "VB", "GND", 1
Delay 100
Switch "KLin", "GND", 1
Delay 30
Switch "KLin", "GND", 0

Delay 100
DCV
Answer "DCV"
MUX "OSW", 0
ClearMem
SetV 0
SwitchOffAll
End Sub
