[STEP1]
S0 = 0000
D0 = 초기상태 ( Power OFF)
Sub Step1()

ClearMem
SetV 0
SwitchOffAll

DELAY 100
Answer ""
End Sub

[STEP2]
S0 = 0000
D0 = 암전류 측정

Sub Step2()
MUX "DARK", 1
DELAY 500
SetV 13.5
Delay 2500
'msgbox "1"
RESULT = DCI("DARK", 0)
Answer "DCI_DARK"
MUX "DARK", 0

End Sub

[STEP3]
S0 = 1002
D0 = IG입력검사
Sub Step3()
MUX "OSW", 1
Switch "KLin", "GND", 1
DELAY 1000
Switch "KLin", "GND", 0

SetV 13.5
Switch "VB", "GND", 1
Switch "IG", "POW", 1
Switch "KLin", "MCU_KLINE", 1
Delay 300
'msgbox "2"
DCV
Answer "DCV"
MUX "OSW", 0
End Sub

[STEP4]
S0 = 1003
D0 = normal session
Sub Step4()

Switch "IG", "POW", 1
Switch "KLin", "MCU_KLINE", 1
Answer ""
End Sub

[STEP5]
S0 = 1004
D0 = Connection
Sub Step5()

OpenKLine(1)
'Result = K_Session
'Result = K_Test
Result = K_FncTest
Result = K_RequestSeed
Answer  "SEED"
End Sub

[STEP6]
S0 = 1005
D0 = ROM ID Check
Sub Step6()
Result = K_ReadEcu(1)
DELAY 50
Result = K_ReadEcu(2)
Answer  "CODE_ID"
End Sub

[STEP7]
S0 = 1006
D0 = Check Sum 확인
Sub Step7()
DELAY 400
Result = K_ReadEcu(3)
DELAY 200
Result = K_ReadEcu(4)
Answer  "CODE_CHECKSUM"
End Sub

[STEP8]
S0 = 1007
D0 = Ecu variation Number
Sub Step8()
Delay 200
Result = K_ReadEcu(5)
Answer  "VARIATION"
End Sub

[STEP9]
S0 = 1100
D0 = function session - Session 변경(이제 필요 없음)
Sub Step9()
Switch "VB", "GND", 1
