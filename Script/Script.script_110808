[STEP17]
S0 = 3000
D0 = SSW Check1
Sub Step17()

Switch "11", "4", 1
Switch "CSW", "GND", 0
Switch "SSW", "GND", 0
Switch "TSW", "GND", 0

DELAY 200
Result = K_ReadFunction
Answer "SW_CODE"

Switch "11", "4", 0
End Sub

