End Sub

[Step40]
S0 = 5000
D0 = Power Latch On Check
Sub Step40()
MUX "OSW", 1
'Switch "VB", "GND", 1
'Switch "IG", "POW", 1
Switch "IG", "POW", 0
DELAY 50
RESULT = K_ReadFunction
DCV
Answer "DCV"
End Sub

[Step41]
S0 = 5000
D0 = Power Latch Off Check (오류)
Sub Step41()
'MUX "OSW", 1
RESULT = K_WriteFunction(3, "OFF")      ' PWL Relay Off
RESULT = K_WriteFunction(4, "OFF")      ' NSLP Relay Off
'RESULT = K_ReadFunction
DELAY 100
DCV
Answer "DCV"
'MUX "OSW", 0
End Sub

[Step42]
S0 = 1001
D0 = INH 출력 확인
Sub Step42()
'MUX "OSW", 1
'SetV 0
'DELAY 100
'SwitchOffAll
'
'DELAY 100
'SetV 13.5
'Switch "VB", "GND", 1
'DELAY 300
Switch "KLin", "GND", 1
DELAY 50
Switch "KLin", "GND", 0
DELAY 10
DCV
Answer "DCV"

SwitchOffAll
ClearMem
SetV 0
End Sub
