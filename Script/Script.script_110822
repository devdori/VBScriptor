[Step1]
S0 = 0
D0 = 초기상태 ( Power OFF)
Sub Step1()
ClearMem
SetV 0
'Switch "VB", "GND", 0
'Switch "IG", "POW", 0
SwitchOffAll
'SetV 13.5
'Switch "VB", "GND", 1
'DELAY 300
'Switch "IG", "POW", 1
'DELAY 300
Answer ""
End Sub

[Step2]
S0 = 0
D0 = 암전류 측정
Sub Step2()
MUX "DARK", 1
'DELAY 100
SetV 13.5
DELAY 1800
'msgbox "1"
RESULT = DCI("DARK", 0)
Answer "DCI_DARK"
MUX "DARK", 0
SetV 0
End Sub

[Step3]
S0 = 1002
D0 = IG입력검사
Sub Step3()
MUX "OSW", 1
Switch "VB", "GND", 1
Switch "IG", "POW", 1
SetV 13.5
DELAY 100
'msgbox "2"
DCV
Answer "DCV"
MUX "OSW", 0
End Sub

[Step4]
S0 = 1003
D0 = normal session
Sub Step4()
'Switch "VB", "GND", 1
'Switch "IG", "POW", 1
Switch "KLin", "MCU_KLINE", 1
'Delay 100
Answer ""
'Delay 100
End Sub

[Step5]
S0 = 1004
D0 = Connection
Sub Step5()
'Switch "VB", "GND", 1
'Switch "IG", "POW", 1
'msgbox "wi"
OpenKLine (1)
'Result = K_Session
'Result = K_Test
RESULT = K_FncTest
RESULT = K_RequestSeed
'Delay 50
Answer "SEED"
'Delay 100
End Sub

[Step6]
S0 = 1005
D0 = ROM ID Check
Sub Step6()
RESULT = K_ReadEcu(1)
'DELAY 100
'Result = K_ReadEcu(2)
Answer "CODE_ID"
End Sub

